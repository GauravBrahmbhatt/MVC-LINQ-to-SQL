@model IEnumerable<TicketApp_usingEF.Ticket>

@{
    Layout = null;
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <link href="~/bootstrap-3.3.6-dist/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/jquery-1.12.0.min.js"></script>
    <title>IndexView</title>
</head>
<body>

    <h1><br /><br /><u><b><p style="text-align:center">@Html.ActionLink("Conjure ticket", "AddTicket")</p></b></u></h1>
    <br /><br />

    <table class="table" border="1">
        <tr>
            <th>User Name</th>
            <th>Date Opened</th>
            <th>Status</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(x => item.Username)</td>
                <td>@Html.DisplayFor(x => item.IsClosed)</td>
                <td>@Html.DisplayFor(x => item.DateOpened) </td>
            </tr>
        }
    </table>
</body>
</html>


